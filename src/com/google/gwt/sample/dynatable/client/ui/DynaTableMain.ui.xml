<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.google.gwt.sample.dynatable.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<table width="100%" border="0"
			summary="School Schedule for Professors and Students">
			<tr valign="top">
				<td id="calendar" align="center" width="90%">
					<s:SchoolCalendarWidget ui:field="calendar" />
				</td>
				<td id="days" align="center" width="10%">
					<g:ListBox ui:field="pageSize">
						<g:item value="15">15</g:item>
						<g:item value="30">30</g:item>
						<g:item value="50">50</g:item>
					</g:ListBox>
					<s:DayFilterWidget ui:field="filter" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 